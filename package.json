  "main": "index.js",
  "scripts": {
    "start": "nodemon ./server.js",
    "test": "mocha --exit",
    "watch-js": "nodemon -e js --watch public/js -x \"npm run build-js\"",
    "watch-sass": "nodemon -e scss --watch public/css -x \"npm run build-sass\"",
    "watch": "npm run watch-js && npm run watch-sass",
    "watch-jscss": "gulp watch",
    "start-local-server": "npm-run-all --parallel watch-jscss start"
  },
