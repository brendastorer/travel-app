  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2"
  }
